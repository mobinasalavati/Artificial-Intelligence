    "def h3(n, m, node):\n",
    "    \"\"\"Euclidean distance to nearest Fellowship member\"\"\"\n",
    "    location = node.state[0]\n",
    "    fellowships = node.state[1]\n",
    "    \n",
    "    if not fellowships:\n",
    "        return 0\n",
    "    \n",
    "    min_distance = math.sqrt((n**2) + (m**2))  # Maximum possible distance\n",
    "    for fellowship in fellowships:\n",
    "        dx = location[0] - fellowship[0][0]\n",
    "        dy = location[1] - fellowship[0][1]\n",
    "        distance = math.sqrt(dx*dx + dy*dy)\n",
