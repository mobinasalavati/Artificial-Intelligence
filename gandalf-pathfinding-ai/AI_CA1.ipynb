   "id": "51bd7ccf",
   "metadata": {},
   "outputs": [],
   "source": [
    "def init_table(n, m, orcs):\n",
    "    table = [['_'] * m for _ in range(n)]\n",
    "    \n",
    "    for (x, y), grade in orcs:\n",
    "        # Mark orc surveillance area\n",
    "        for dx in range(-grade, grade + 1):\n",
    "            for dy in range(-grade, grade + 1):\n",
    "                if abs(dx) + abs(dy) < grade:  # Manhattan distance\n",
    "                    nx, ny = x + dx, y + dy\n",
    "                    if 0 <= nx < n and 0 <= ny < m:\n",
    "                        table[nx][ny] = '*'\n",
    "    return table"
   ]
