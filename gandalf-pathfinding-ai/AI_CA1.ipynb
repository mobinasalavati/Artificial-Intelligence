  {
   "cell_type": "code",
   "execution_count": null,
   "id": "2f612da6",
   "metadata": {},
   "outputs": [],
   "source": [
    "def to_find_path(node):\n",
    "    path_actions = []\n",
    "    while node.parent != -1:\n",
    "        path_actions.append(node.action)\n",
    "        node = node.parent\n",
    "    path_actions.reverse()\n",
    "    return ''.join(path_actions)"
   ]
